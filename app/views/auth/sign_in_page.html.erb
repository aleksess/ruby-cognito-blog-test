<h1>Sign in</h1>

<%= form_with url: '/auth/sign_in', method: 'POST', model: @credentials do | form |%>
    <div>
        <%= form.label :Username %><br>
        <%= form.text_field :username %>
    </div>

    <div>
        <%= form.label :Password %><br>
        <%= form.password_field :password %>
    </div>

    <%= form.submit "Sign In"%>

<% end %>